{
  "name": "guardian-dental-dashboard",
  "version": "1.0.0",
  "description": "Guardian Dental Billing - Employee Dashboard and Management System",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "seed": "node seedDatabase.js",
    "seed:prod": "NODE_ENV=production node seedDatabase.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "lint": "eslint . --fix",
    "build": "echo 'Static files ready for deployment'",
    "health": "curl -s http://localhost:5000/api/health || echo 'Server not running'"
  },
  "keywords": [
    "dental-practice-management",
    "employee-dashboard",
    "guardian-dental-billing",
    "healthcare-management",
    "attendance-tracking",
    "admin-panel",
    "mongodb",
    "express",
    "production-ready"
  ],
  "author": "Guardian Dental Billing LLC",
  "license": "PROPRIETARY",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.4.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
